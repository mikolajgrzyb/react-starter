{
  "name": "react-starter",
  "version": "0.0.1",
  "description": "Seed project that will allow to build react pages fast and easy.",
  "main": "index.html",
  "scripts": {
    "serve": "webpack-dev-server --inline --progress",
    "clean": "rimraf dist build",
    "build:dev": "npm run clean && webpack --config config/webpack.dev.js --progress --profile --bail",
    "build:prod": "npm run clean && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "deploy": "npm run build:prod && node scripts/deploy.js",
    "start": "npm run serve"
  },
  "keywords": [
    "landing",
    "seed",
    "starter",
    "webpack"
  ],
  "author": "Mikolaj Grzyb",
  "license": "MIT",
  "devDependencies": {
    "aws-sdk": "2.7.21",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-preset-es2015": "https://registry.npmjs.org/babel-preset-es2015/-/babel-preset-es2015-6.18.0.tgz",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.15",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "html-webpack-harddisk-plugin": "0.0.2",
    "html-webpack-plugin": "^2.26.0",
    "image-webpack-loader": "^2.0.0",
    "mime-types": "^2.1.13",
    "node-env-file": "^0.1.8",
    "node-sass": "^4.2.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss-loader": "^1.2.2",
    "pug": "^2.0.0-beta6",
    "pug-loader": "^2.3.0",
    "resolve-url-loader": "^1.6.1",
    "sass-lint": "^1.10.2",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-livereload-plugin": "^0.10.0",
    "webpack-merge": "^2.4.0"
  },
  "dependencies": {
    "history": "^4.6.1",
    "img-loader": "^1.3.1",
    "normalize.css": "^5.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "rimraf": "^2.5.4",
    "universal-router": "^3.0.0"
  }
}
